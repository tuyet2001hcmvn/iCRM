@model IEnumerable<FileAttachmentViewModel>
<div class="content-header clearfix">
    <h1 class="pull-left">
        @LanguageResource.Customer_FileAttachment
    </h1>
    <div class="pull-right">
        <a class="btn bg-blue" id="btn-add-fileAttachment"><i class="fa fa-plus-square"></i> Tải lên</a>
    </div>
</div>
<div class="content overflow-x-scroll" id="contentFileAttachment">
    <table class="table table-bordered table-striped table-hover no-footer">
        <thead class="isd-table-header">
            <tr>
                <th class="text-center isd-table-th-stt">@LanguageResource.NumberIndex</th>
                <th>@Html.DisplayNameFor(p => p.FileType)</th>
                <th>@Html.DisplayNameFor(p => p.FileAttachmentName)</th>
                <th>@Html.DisplayNameFor(p => p.CreateUser)</th>
                <th>@Html.DisplayNameFor(p => p.CreateTime)</th>
                <th class="text-center isd-table-th-action">@LanguageResource.Actions</th>
            </tr>
        </thead>
        <tbody>
            @Html.Partial("_ListContent")
        </tbody>
        <tfoot></tfoot>
    </table>
</div>