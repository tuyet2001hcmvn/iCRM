        cssCollapsedInvestor = "collapsed-box";
        cssDisplayNoneInvestor = "display: none;";
    }
    else
    {
        cssfaInvestor = "fa fa-minus";
    }
    //Tư vấn-Thiết kế
    var designList = (List<OpportunityPartnerViewModel>)ViewBag.Design;
    if (designList == null || designList.Count == 0)
    {
        cssfaDesign = "fa fa-plus";
        cssCollapsedDesign = "collapsed-box";
        cssDisplayNoneDesign = "display: none;";
    }
    else
    {
        cssfaDesign = "fa fa-minus";
    }
    //Tổng thầu
    var contractorList = (List<OpportunityPartnerViewModel>)ViewBag.Contractor;
