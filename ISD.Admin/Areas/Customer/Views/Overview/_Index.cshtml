        cssCollapsedContractor = "collapsed-box";
        cssDisplayNoneContractor = "display: none;";
    }
    else
    {
        cssfaContractor = "fa fa-minus";
    }
    //Đơn vị tư vấn giám sát
    var consultingList = (List<OpportunityPartnerViewModel>)ViewBag.Consulting;
    if (consultingList == null || consultingList.Count == 0)
    {
        cssfaConsulting = "fa fa-plus";
        cssCollapsedConsulting = "collapsed-box";
        cssDisplayNoneConsulting = "display: none;";
    }
    else
    {
        cssfaConsulting = "fa fa-minus";
    }
    //Thi công
    var internalList = (List<OpportunityPartnerViewModel>)ViewBag.Internal;
    var competitorList = (List<OpportunityPartnerViewModel>)ViewBag.Competitor;
    if ((internalList == null || internalList.Count == 0) || (competitorList == null || competitorList.Count == 0))
    {
        cssfaConstruction = "fa fa-plus";
        cssCollapsedConstruction = "collapsed-box";
        cssDisplayNoneConstruction = "display: none;";
    }
    else
    {
        cssfaConstruction = "fa fa-minus";
    }
    //Đối thủ
    var oppCompetitorList = (List<OpportunityPartnerViewModel>)ViewBag.OppCompetitor;
    if (oppCompetitorList == null || oppCompetitorList.Count == 0)
    {
        cssfaOppCompetitor = "fa fa-plus";
        cssCollapsedOppCompetitor = "collapsed-box";
        cssDisplayNoneOppCompetitor = "display: none;";
    }
    else
    {
        cssfaOppCompetitor = "fa fa-minus";
    }

}
