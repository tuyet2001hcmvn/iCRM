    var profileGroupList = Model.profileGroupList;

    var opportunityActivitiesList = (List<ProjectActivitiesViewModel>)ViewBag.OpportunityActivities;

    var productWarrantyList = (List<ProductWarrantyViewModel>)ViewBag.ProductWarranty;
    if (productWarrantyList == null || productWarrantyList.Count == 0)
    {
        cssfaProductWarranty = "fa fa-plus";
        cssCollapsedProductWarranty = "collapsed-box";
        cssDisplayNoneProductWarranty = "display: none;";
    }
    else
    {
        cssfaProductWarranty = "fa fa-minus";
    }

    var ticketList = (List<TaskViewModel>)ViewBag.Ticket;
    if (ticketList == null || ticketList.Count == 0)
    {
        cssfaTicket = "fa fa-plus";
        cssCollapsedTicket = "collapsed-box";
        cssDisplayNoneTicket = "display: none;";
    }
    else
    {
        cssfaTicket = "fa fa-minus";
    }
    var gtbList = (List<TaskViewModel>)ViewBag.GTB;
