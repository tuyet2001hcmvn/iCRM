    var activitiesList = (List<TaskViewModel>)ViewBag.Activities;
    var missionList = (List<TaskViewModel>)ViewBag.Mission;

    if ((appointmentList == null || appointmentList.Count == 0) && (activitiesList == null || activitiesList.Count == 0) && (missionList == null || missionList.Count == 0))
