    string CurrentArea = ConstArea.Reports;
    string CurrentController = ConstController.ProductWarrantyReport;

    string CurrentUrl = HtmlExtensions.GetCurrentUrl(CurrentArea, CurrentController);
    ViewBag.Title = "Báo cáo đăng ký bảo hành";
    var listSystemTemplate = (List<PivotTemplateViewModel>)ViewBag.SystemTemplate;
    var listUserTemplate = (List<PivotTemplateViewModel>)ViewBag.UserTemplate;
    var templateId = ViewBag.TemplateId;
    var modeSearch = ViewBag.ModeSearch;
    ViewBag.CurrentUrl = CurrentUrl;

    var ERPProductCodeList = (List<string>)ViewBag.ERPProductCodeList;
    var SerialNumberList = (List<string>)ViewBag.SerialNumberList;
    var SaleOrderList = (List<string>)ViewBag.SaleOrderList;
    var OrderDeliveryList = (List<string>)ViewBag.OrderDeliveryList;
    var ProfileCodeList = (List<string>)ViewBag.ProfileCodeList;
}

<div class="content-header clearfix">
    <h1 class="pull-left">
        @ViewBag.Title
    </h1>
    <div class="pull-right">
        @Html.Partial("~/Areas/Reports/Views/_PivotTemplateHandle.cshtml")
